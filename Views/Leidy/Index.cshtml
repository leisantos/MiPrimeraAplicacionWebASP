<section class="section about-section gray-bg" id="about">
            <div class="container">
                <div class="row align-items-center flex-row-reverse">
                    <div class="col-lg-6">
                        <div class="about-text go-to">
                            <h3 class="dark-color">@Model.nombre</h3>
                            <h6 class="theme-color lead">@Model.info</h6><br>
                            <div class="row about-list">
                                <div class="col-md-6">
                                    <div class="media">
                                        <label>Nombre: </label>
                                        <p><br> @Model.nombre</p>
                                    </div>
                                    <div class="media">
                                        <label>Sexo: </label>
                                        <p><br> @Model.sexo</p>
                                    </div>
                                    <div class="media">
                                        <label>Direccion: </label>
                                        <p><br> @Model.direccion</p>
                                    </div>
                                    <div class="media">
                                        <label>Ciudad: </label>
                                        <p><br> @Model.ciudad</p>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="media">
                                        <label>E-mail: </label>
                                        <p><br> @Model.correo</p>
                                    </div>
                                    <div class="media">
                                        <label>Telefono: </label>
                                        <p><br> @Model.telefono</p>
                                    </div>
                                    <div class="media">
                                        <label>Edad: </label>
                                        <p><br> @Model.edad a&ntilde;os</p>
                                    </div>
                                    <div class="media">
                                        <label>Carrera: </label>
                                        <p><br> @Model.carrera</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="about-avatar" >
                            <img margin="0" padding="0" height="350" width="350" src="~/img/enaleidy.jpg">
                        </div>
                    </div>
                </div>
            </div>
        </section>